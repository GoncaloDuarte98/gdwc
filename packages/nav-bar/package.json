    "@web/dev-server": "^0.1.21",
    "@web/test-runner": "next",
    "concurrently": "^5.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "customElements": "custom-elements.json",
  "gitHead": "8ee6899714eb90426e1d70830fe96730e89f3bc6"
